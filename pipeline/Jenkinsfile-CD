@Library('shared-library') _
pipeline {
  agent any
  environment {
    azcon = credentials('azure-pasha')
  }
   
   stages {   
    
    stage('Infra Provisioning') {
      steps {
        Infra()
      }
    }
    
    
    stage('Download Artifact Form Nexus') {
      steps {
        DownloadFromNexus()
      }
      }
    stage('Deploy To WebApp') {   
      steps{
        
        DeployToWebApp()
       
       }
       }
    }
}
