@Library('github.com/ranjitaws2020/java-shared-library@pipeline') _

pipeline {
    agent any 
    tools {
        maven 'maven-home'
        jdk 'jdk8'
        
    }
    stages {
        stage('Pull the file off Nexus') {
            steps{
                  DownloadFromNexus()                    
            }
        }
        stage ('Deploy on this Server') {
            steps{
                DeployToTomcat()
            }            
        }
    }
}
